def count_tuesdays(N):
    if N < 2:
        return 0
    # Calculate the number of Tuesdays in N days
    return (N - 2) // 7 + 1

# Read number of test cases
T = int(input())
results = []

for _ in range(T):
    N = int(input())
    results.append(count_tuesdays(N))

# Output results
for result in results:
    print(result)
